[rl]
gamma = 0.99

[om]
cell_num = 4
cell_size = 1
om_channel_size = 3

[action_space]
kinematics = holonomic
speed_samples = 5
rotation_samples = 16
sampling = exponential
query_env = true

[cadrl]
mlp_dims = 150, 100, 100, 1
multiagent_training = false

[lstm_rl]
global_state_dim = 100
mlp1_dims = 256, 256, 256, 100
mlp2_dims = 256, 256, 256, 1
multiagent_training = true
with_om = false
with_interaction_module = false

[srl]
mlp1_dims = 256, 256, 256, 100
mlp2_dims = 256, 256, 256, 1
multiagent_training = true
with_om = false

[sarl]
mlp1_dims = 256, 256
mlp2_dims = 256, 100
attention_dims = 256, 256, 1
mlp3_dims = 256, 256, 256, 1
multiagent_training = true
with_om = false
with_global_state = true

[sim]
robot_num = 8
human_num = 5